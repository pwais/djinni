@extern "../byte_buffers.yaml"
@import "../jnativearray.djinni"

refcounted_buffer = interface +c +j {
  reset();
  accept(buf: jheaparray);
}

byte_sort = interface +c {
  static sort_in_place(view: buffer_view);
  
  static consume_and_print_sorted(buf: refcounted_buffer);

  static gen_buffer(size: i64): refcounted_buffer;
}

